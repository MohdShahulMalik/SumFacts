#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/zakir/OneDrive/Documents/AI-News-Summariser-main/news-app-backend/node_modules/.pnpm/jasmine@5.7.0/node_modules/jasmine/bin/node_modules:/mnt/c/Users/zakir/OneDrive/Documents/AI-News-Summariser-main/news-app-backend/node_modules/.pnpm/jasmine@5.7.0/node_modules/jasmine/node_modules:/mnt/c/Users/zakir/OneDrive/Documents/AI-News-Summariser-main/news-app-backend/node_modules/.pnpm/jasmine@5.7.0/node_modules:/mnt/c/Users/zakir/OneDrive/Documents/AI-News-Summariser-main/news-app-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/zakir/OneDrive/Documents/AI-News-Summariser-main/news-app-backend/node_modules/.pnpm/jasmine@5.7.0/node_modules/jasmine/bin/node_modules:/mnt/c/Users/zakir/OneDrive/Documents/AI-News-Summariser-main/news-app-backend/node_modules/.pnpm/jasmine@5.7.0/node_modules/jasmine/node_modules:/mnt/c/Users/zakir/OneDrive/Documents/AI-News-Summariser-main/news-app-backend/node_modules/.pnpm/jasmine@5.7.0/node_modules:/mnt/c/Users/zakir/OneDrive/Documents/AI-News-Summariser-main/news-app-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/jasmine@5.7.0/node_modules/jasmine/bin/jasmine.js" "$@"
else
  exec node  "$basedir/../.pnpm/jasmine@5.7.0/node_modules/jasmine/bin/jasmine.js" "$@"
fi
