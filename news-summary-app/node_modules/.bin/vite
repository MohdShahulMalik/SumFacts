#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/maxum/projects/personal/javascript/SumFacts/news-summary-app/node_modules/.pnpm/vite@6.2.6_@types+node@22.15.4_sass@1.86.3/node_modules/vite/bin/node_modules:/home/maxum/projects/personal/javascript/SumFacts/news-summary-app/node_modules/.pnpm/vite@6.2.6_@types+node@22.15.4_sass@1.86.3/node_modules/vite/node_modules:/home/maxum/projects/personal/javascript/SumFacts/news-summary-app/node_modules/.pnpm/vite@6.2.6_@types+node@22.15.4_sass@1.86.3/node_modules:/home/maxum/projects/personal/javascript/SumFacts/news-summary-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/maxum/projects/personal/javascript/SumFacts/news-summary-app/node_modules/.pnpm/vite@6.2.6_@types+node@22.15.4_sass@1.86.3/node_modules/vite/bin/node_modules:/home/maxum/projects/personal/javascript/SumFacts/news-summary-app/node_modules/.pnpm/vite@6.2.6_@types+node@22.15.4_sass@1.86.3/node_modules/vite/node_modules:/home/maxum/projects/personal/javascript/SumFacts/news-summary-app/node_modules/.pnpm/vite@6.2.6_@types+node@22.15.4_sass@1.86.3/node_modules:/home/maxum/projects/personal/javascript/SumFacts/news-summary-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
